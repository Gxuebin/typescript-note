{
  "title": "Typescript 小书",
  "description": "Typescript 知识梳理",
  "author": "ruizhengyun <ruizhengyun@gmail.com>",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "links": {
    "sidebar": {
      "我的博客": "http://ruizhengyun.cn/",
      "我的小书": "http://ruizhengyun.cn/book"
    }
  },
  "plugins": [
    "popup",
    "insert-logo",
    "-lunr",
    "-search",
    "search-pro",
    "github",
    "chapter-fold",
    "flexible-alerts",
    "splitter",
    "code",
    "todo",
    "emphasize",
    "-sharing",
    "sharing-plus",
    "auto-scroll-table",
    "custom-favicon",
    "lightbox",
    "anchor-navigation-ex",
    "tbfed-pagefooter"
  ],
  "pluginsConfig": {
    "favicon": "http://ruizhengyun.cn/images/wx/favicon.ico",
    "github": {
      "url": "https://github.com/ruizhengyun/typescript-note"
    },
    "insert-logo": {
      "url": "http://ruizhengyun.cn/images/wx/logo.png",
      "style": "background: none; max-height: 66px; min-height: 66px"
    },
    "sharing": {
      "qq": true,
      "qzone": false,
      "weibo": false,
      "douban": false,
      "all": [
        "qq",
        "qzone",
        "weibo",
        "douban"
      ]
    },
    "flexible-alerts": {
      "style": "callout",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    },
    "anchor-navigation-ex": {
      "associatedWithSummary":false,
      "showLevel":true,
      "multipleH1": true,
      "mode": "float",
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy ruizhengyun.cn 2019",
      "modify_label": "修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  }
}
